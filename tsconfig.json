{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"],
      "@assets/css/*":["./assets/css/*"],
      "@assets/js/*":["./assets/js/*"],
      "@assets/image/*":["./assets/images/*"],
      "@assets/public/*":["./public/*"],
      "@assets/svg/*":["./public/svgs/*"],
      "@assets/img/*":["./public/images/*"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/(Admin)/user_table.tsx", "app/(Admin)/users.tsx", "app/(Admin)/AdminGalleryPage", "app/(Admin)/materialPage/page.tsx"],
  "exclude": ["node_modules"]
}
